{
    "Name": "CreateFolder",
    "Technology": "Python",
    "Description": "Creates folder",
    "Version": "1.0.0",
    "Category": "FOLDER",
    "Inputs": [
        {
            "Name": "folderToBeCreated",
            "Type": "string",
            "DefaultValue": "NA",
            "Description": "Name of the folder to be created."
        },
        {
            "Name": "sourceFolderPath",
            "Type": "string",
            "DefaultValue": "NA",
            "Description": "Location of folder where new folder has to be created."
        }
    ],
    "Outputs": [
        {
            "Name": "status",
            "Type": "string",
            "Description": "Status of the process execution."
        },
        {
            "Name": "createdFolderPath",
            "Type": "string",
            "Description": "Folder path of the created folder."
        }
    ],
    "Dependencies": [
        {
            "Name": "os",
            "Type": "module",
            "Description": "In-built library to interact with system OS."
        }
    ],
    "Authors": [
        {
            "Name": "Ankit Mishra",
            "Email": "ankit.mishra@robofi.com",
            "Contact": "+91 9713490290"
        }
    ],
    "Errors": [
        {
            "Message": "SOURCE_FOLDER_IS_NULL",
            "Code": "0000000021",
            "Description": "Source folder value is null."
        },
        {
            "Message": "SOURCE_FOLDER_NOT_FOUND",
            "Code": "0000000022",
            "Description": "Source folder does not exist."
        }
    ]
}